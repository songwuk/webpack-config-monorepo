  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "babel-jest": "^28.1.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.3",
    "head-webpack-plugin": "workspace:*",
    "html-webpack-plugin": "4",
    "jest": "^28.1.0",
    "memfs": "^3.4.7",
    "memory-fs": "^0.5.0",
    "webpack": "4.46.0",
    "webpack-cli": "3.x",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "html-loader": "^3.1.0"
